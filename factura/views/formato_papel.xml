<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="paperformat_letter_size" model="report.paperformat">
            <field name="name">Media Carta (Half Letter)</field>
            <field name="default" eval="False"/>
            <field name="format">custom</field>
            <field name="page_height" eval="139.7"/> 
            <field name="page_width" eval="215.9"/> 
            <field name="orientation">Portrait</field>
            <field name="margin_top" eval="10"/>
            <field name="margin_bottom" eval="15"/>
            <field name="margin_left" eval="5"/>
            <field name="margin_right" eval="5"/>
            <field name="header_line" eval="False"/>
            <field name="header_spacing" eval="10"/>
            <field name="code">half_letter</field>
        </record>   
    </data>
</odoo>
